<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <SolidColorBrush x:Key="Brush.Status.Ok" Color="#4CAF50" />
    <SolidColorBrush x:Key="Brush.Status.Warning" Color="#FFC107" />
    <SolidColorBrush x:Key="Brush.Status.Error" Color="#F44336" />
    <SolidColorBrush x:Key="Brush.Status.Offline" Color="#9E9E9E" />
    <SolidColorBrush x:Key="Brush.Layout.Header" Color="#1F1F1F" />
    <SolidColorBrush x:Key="Brush.Layout.Sidebar" Color="#2D2D30" />

    <SolidColorBrush x:Key="Brush.Overlay.Background" Color="#80000000" />
</ResourceDictionary>